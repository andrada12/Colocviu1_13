<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="match_parent"
android:layout_height="match_parent"
tools:context=".Colocviu1_13SecondaryActivity">




<TextView
    android:id="@+id/cardinals_number_edit_text"
    android:layout_height="wrap_content"
    android:layout_width="match_parent"
    android:inputType="text"
    android:enabled="false"
    android:gravity="center"
    android:layout_alignParentRight="true"
    android:layout_alignParentEnd="true"
    android:text=""/>

<Button
    android:id="@+id/register_button"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_alignLeft="@+id/cardinals_number_edit_text"
    android:layout_alignStart="@+id/cardinals_number_edit_text"
    android:layout_below="@+id/cardinals_number_edit_text"
    android:text="@string/register"/>

<Button
    android:id="@+id/cancel_button"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_alignRight="@+id/cardinals_number_edit_text"
    android:layout_alignEnd="@+id/cardinals_number_edit_text"
    android:layout_below="@+id/cardinals_number_edit_text"
    android:text="@string/cancel"/>

</RelativeLayout>
